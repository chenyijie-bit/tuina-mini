@keyframes warn1 {
	0% {
	transform: scale(1);
	-webkit-transform: scale(1);
	opacity: 0.0;
	}
	25% {
	transform: scale(1.3);
	-webkit-transform: scale(1.3);
	opacity: 0.2;
	}
	50% {
	transform: scale(1.5);
	-webkit-transform: scale(1.5);
	opacity: 0.3;
	}
	75% {
	transform: scale(1.8);
	-webkit-transform: scale(1.8);
	opacity: 0.4;
	}
	100% {
	transform: scale(2);
	-webkit-transform: scale(2);
	opacity: 0.0;
	}
}
.pop-container {
	display: flex;
	justify-content: center;
	align-items: center;
	position: relative;
	width: 180rpx;
	height: 180rpx;
	margin: auto;
	margin-top: 80rpx;
}
/* 产生动画（向外扩散变大）的圆圈 第一个圆 */
.pulse,.pulse1,.pulse2,.pulse3 {
	position: absolute;
	width: 100rpx;
	height: 100rpx;
	left: 80;
	top: 80;
	border: 4rpx solid #DFAF3C;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	z-index: 1;
	opacity: 0;
	/* animation: warn1 1.5s linear 0.1s infinite; */
	box-shadow: 3px 3px 30px #DFAF3C; 
}
/* 产生动画（向外扩散变大）的圆圈 第二个圆 */
.pulse1 {
	animation: warn1 1s linear 0.1s infinite;
}
.pulse2 {
	animation: warn1 1s linear .3s infinite;
}
.pulse3 {
	animation: warn1 1s linear .6s infinite;
}
.pulse4 {
	animation: warn1 1s linear 1s infinite;
}
