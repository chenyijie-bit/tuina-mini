/* pages/wodeyeji/index.wxss */
page{
  padding: 20rpx;
    box-sizing: border-box;
}
.width3th{
  display: flex;
  justify-content: space-around;
  text-align: center;
  padding: 12rpx 0;
}
.width3th view{
  flex: 1;
}
.width3th:nth-child(2n){
  background-color: #efefef;
}
.custom-date{
  display: flex;
    justify-content: space-between;
    padding: 20rpx 30rpx;
    margin-bottom: 40rpx;
    margin-top: 40rpx;
    background: #efefef;
    border-radius: 12rpx;
}
.but-but button{
  color: #BA9245 !important;
  border-color: #BA9245;
}