page{
	background: rgba(247, 248, 249, 1);
	height: 120%;
	width:100%;
	overflow-x:hidden;
  }
  .van-tabs__wrap{
	position: fixed;
	width: 100% !important;
  }
  .van-tabs__nav .van-ellipsis{
	font-size: 30rpx;
	font-family: PingFang SC;
	font-weight: 500;
	color: #282828;
	opacity: 0.8;
  }
  .van-tab--active .van-ellipsis{
	font-weight: bold;
	color: #BA9245;
  }
  .van-tabs__nav .van-tabs__line{
	background-color: #BA9245;
	width: 140rpx !important;
	height: 6rpx !important;
	}
	.tab-box{
		padding: 130rpx 20rpx;
	}
	.tab-items{
		touch-action: none;
		margin-bottom: 40rpx;
	}
	.demo-swipe-cell {
		user-select: none;
	}
	
	.van-swipe-cell__left,
	.van-swipe-cell__right {
		display: inline-block;
		width: 65px;
		height: 44px;
		font-size: 15px;
		line-height: 44px;
		color: #fff;
		text-align: center;
		display: flex;
		justify-content: center;
		align-items: center;
		background-color: #ee0a24;
	}
.order-contain{
	background: #fff;
	padding: 45rpx;
	box-shadow: 0px 4px 20px 0px rgba(192, 194, 200, 0.35);
	border-radius: 24rpx;
	display: flex;
}
.info-box{
	flex: 1;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
}
.but-box{
	width: 190rpx;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	padding-top: 30rpx;
	padding-bottom: 30rpx;
}
.but-box .nouser-but{
	opacity: .6;
}
.marbot28{
	margin-bottom: 28rpx;
}
.but-box button{
	padding: 0 10rpx;
	height: 60rpx;
    font-size: 26rpx;
	width: 190rpx;
	background: #BA9245;
    border: none;
    border-radius: 8rpx;
    color: #fff;
	font-weight: 500;
}
.order-contain view text:nth-child(1){
	font-size: 26rpx;
font-family: PingFang SC;
font-weight: 500;
color: #282828;
}
.order-contain view text:nth-child(2){
	font-size: 26rpx;
font-family: PingFang SC;
font-weight: 500;
color: #BA9245;
}

.van-dialog-id .van-dialog__button button,.van-dialog-id-sec .van-dialog__button button{
		color: #BA9245;
}
.van-dialog-id-sec  .van-dialog__header{
	padding-bottom: 20rpx;
    padding-top: 30rpx;
}
.van-cell .van-cell__title{
	min-width: 192rpx !important;
	margin-right: 6rpx !important;
}
.van-dialog-id-sec-shadow{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 100;
	background-color: var(--overlay-background-color,rgba(0,0,0,.7));
}
.van-dialog-id-sec-shadow .van-dialog-id-sec{
	width: 640rpx;
	background: #fff;
	min-height: 300rpx;
	margin: 160rpx auto;
  border-radius: 40rpx;
}
.van-dialog-id-sec .title{
	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
	font-weight: 500;
}
.dialog-but-box{
	display: flex;
	height: 100rpx;
	line-height: 100rpx;
}
.dialog-but-left,.dialog-but-right{
	flex:1;
	text-align: center;
}
.dialog-but-left{
	border-right: 1rpx solid #efefef;
}
.dialog-but-right{
	color: #BA9245;
}
.van-swipe-cell__left{
	background: #BA9245;
}